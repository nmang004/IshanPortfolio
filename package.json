{
  "name": "portfolio-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "tsx scripts/migrate-content.ts",
    "migrate:dry-run": "DRY_RUN=true tsx scripts/migrate-content.ts",
    "migrate:rollback": "tsx scripts/migrate-content.ts rollback",
    "migrate:validate": "tsx scripts/migration-validator.ts",
    "analyze": "tsx scripts/analyze-content.ts",
    "analyze:old-site": "tsx scripts/analyze-content.ts ../",
    "migration:setup": "node scripts/setup-migration.js",
    "migration:prepare": "npm run analyze && npm run migrate:dry-run && npm run migrate:validate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:dev": "start-server-and-test dev 3000 'cypress open'",
    "test:visual": "cypress run --spec 'cypress/e2e/visual-regression.cy.ts'",
    "test:a11y": "jest --testPathPattern=accessibility",
    "test:lighthouse": "lhci autorun",
    "test:all": "npm run test:ci && npm run test:e2e && npm run test:lighthouse",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:integration": "jest --testPathPattern=integration"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
